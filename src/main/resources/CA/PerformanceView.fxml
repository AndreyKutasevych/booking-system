<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CA.PerformanceList">
   <children>
      <Label text="select date">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <DatePicker fx:id="dateOfPerformance" />
      <Label text="select time">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="timeChoice" layoutX="100" layoutY="50">
         <items>
            <FXCollections fx:factory="observableArrayList">
               <String fx:value="Matinee" />
               <String fx:value="Evening" />
            </FXCollections>
         </items>
      </ChoiceBox>
      <Button mnemonicParsing="false" onAction="#AddPerformance" text="Add performance" />
      <Accordion fx:id="container"/>
      <Button mnemonicParsing="false" onAction="#Reset" text="Full reset" />
   </children>
</VBox>
